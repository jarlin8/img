#: ../admin/view/autolinks.php:852 ../admin/view/autolinks.php:874
#: ../admin/view/autolinks.php:1263 ../admin/view/autolinks.php:1285
msgid "White Space"
msgstr "白色区域"

#: ../admin/class-daam-admin.php:2381 ../admin/class-daam-admin.php:2414
#: ../admin/view/autolinks.php:854 ../admin/view/autolinks.php:876
#: ../admin/view/autolinks.php:1265 ../admin/view/autolinks.php:1287
